from ultralytics import YOLO
model = YOLO("yolov8n.pt")  # Nano model (smallest & fastest)
model.train(data="C:/Users/lenovo/Desktop/Mini_project/archive/data.yaml", 
            epochs=30, imgsz=640, batch=4, workers=2)
model.val()
